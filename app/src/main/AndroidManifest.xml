<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nexgo.apiv3demo">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".NexgoApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:excludeFromRecents="true"
            android:exported="true">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PinpadActivity" />
        <activity android:name=".PrinterActivity" />
        <activity android:name=".PinpadCustomLayoutActivity" />
        <activity android:name=".DUKPTActivity"></activity>
        <activity android:name=".DesfireActivity"></activity>
        <activity android:name=".EmvActivity2"></activity>
        <activity android:name=".PlatformActivity" />

    </application>

</manifest>